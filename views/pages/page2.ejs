        }

        .all-steps {
            text-align: center;
            margin-top: 30px;
            margin-bottom: 30px;
            width: 100%;
            display: inline-flex;
            justify-content: center
        }

        .step {
            height: 40px;
            width: 40px;
            margin: 0 2px;
            background-color: #bbbbbb;
            border: none;
            border-radius: 50%;
            display: flex;
            justify-content: center;
            align-items: center;
            font-size: 16px;
            color: black;
            opacity: 0.8;
            font-weight: bold;
        }

        .step.active {
            opacity: 1
        }

        .step.finish {
            color: #fff;
            background: #6a1b9a;
            opacity: 1
        }

        .all-steps {
            text-align: center;
            margin-top: 30px;
            margin-bottom: 30px
        }

        .thanks-message {
            display: none
        }

        .custom-input {
            display: flex;
            float: right;
        }

        .custom-input input {
            border: 0;
            border-bottom: 1px solid;
            border-radius: 0;
            height: 10px;
            width: 100px;
        }

        .memory input {
            border: 0;
            width: 50px;
        }

        .attention input {
            width: 75px;
            border: 0;
        }

        .attention .three input {
            width: 45px;
        }

        .attention .custom-input {
            float: left;
        }



        .language .custom-input input {
